      <CodeComponent name="logging_in_every_frame">
        <Param name="Begin Experiment" val="# -*- coding: utf-8 -*-&#10;import numpy as np&#10;import codecs&#10;from datetime import datetime&#10;import random&#10;&#10;song_files = [f for f in os.listdir('songs')]&#10;random.shuffle(song_files)&#10;song_index=0" valType="extendedCode" updates="constant"/>
        <Param name="name" val="logging_in_every_frame" valType="code" updates="None"/>
        <Param name="Each Frame" val="if np.mod(frameN, np.floor((screen_refresh / flicker_freq))) == 0 or frameN == 0:&#10;    letter_counter += 1&#10;    if letter_counter &gt;= len(letters):&#10;        # loop letters&#10;        letter_counter=0&#10;    letter_stimulus.setText(letters[letter_counter])&#10; &#10;if np.mod(frameN, screen_refresh / flicker_freq) &lt; (screen_refresh / flicker_freq) - 5:&#10;    letter_stimulus.draw()        &#10;elif np.mod(frameN, screen_refresh / flicker_freq) == (screen_refresh / flicker_freq) - 5:&#10;    letter_stimulus.setText('')&#10;    logging.exp(&quot;Set letter_stimulus dc&quot;)" valType="extendedCode" updates="constant"/>
        <Param name="Begin Routine" val="n_movieframes = 0&#10;last_movietime = None&#10;lastframe_time = None&#10;movie_frame_increments = []&#10;realtime_frame_increments = []&#10;movie_lags = []&#10;&#10;&#10;#read the story file and prepare the letters to be displayed&#10;filename='songs/'+song_files[song_index]&#10;logging.exp(&quot;Story file displayed: &quot;+str(filename))&#10;storyfile = codecs.open(filename, encoding='utf-8')&#10;real_artist = song_files[song_index].split('-')[0].replace('_', ' ')&#10;fake_artist = real_artist&#10;while fake_artist == real_artist:&#10;    fake_artist = song_files[random.randint(0, len(song_files) - 1)].split('-')[0].replace('_', ' ')&#10;artists = [real_artist, fake_artist]&#10;random.shuffle(artists)&#10;artist_choice_text = &quot;%s oder %s&quot; % tuple(artists)&#10;logging.exp(&quot;artist order: %s&quot; % artists)&#10;&#10;song_index+=1&#10;letters = storyfile.read()&#10;letters = '  '.join(letters.split())+'  '&#10;storyfile.close()&#10;letter_counter=-1&#10;&#10;#letter_stimulus.setText(letters[letter_counter])&#10;screen_refresh = 60.0 # Hz&#10;flicker_freq = 1.5 # Hz" valType="extendedCode" updates="constant"/>
        <Param name="End Routine" val="" valType="extendedCode" updates="constant"/>
        <Param name="End Experiment" val="#import pylab as pl&#10;#pl.subplot(1,2,1)&#10;#pl.hist(realtime_frame_increments, bins=100)&#10;#pl.title('Distance of two movie frames in real time')&#10;#pl.xlabel('Time in secs')&#10;#pl.subplot(1,2,2)&#10;#pl.hist(movie_lags, bins=100)&#10;#pl.title('Lag between movie time and real time for all frames')&#10;#pl.xlabel('Time in seconds')&#10;#print &quot;Movie frames played:&quot;, n_movieframes&#10;#print 'Mean movie frame increment:', np.mean(movie_frame_increments), 's (', 1./np.mean(movie_frame_increments), 'fps )'&#10;#print 'Unique movie frame increments', np.unique(movie_frame_increments)&#10;#pl.show()" valType="extendedCode" updates="constant"/>
